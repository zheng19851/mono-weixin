<?xml version="1.0" encoding="GB2312"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="menuService-hessian" class="org.springframework.remoting.caucho.HessianServiceExporter">
    	<property name="service" ref="remoteMenuService"/>
    	<property name="serviceInterface" value="com.kongur.monolith.weixin.client.menu.RemoteMenuService"/>
	</bean>
   
  <bean id="appEventService-hessian" class="org.springframework.remoting.caucho.HessianServiceExporter">
    	<property name="service" ref="remoteAppEventService"/>
    	<property name="serviceInterface" value="com.kongur.monolith.weixin.client.support.RemoteAppEventService"/>
	</bean>
	
	<bean id="userService-hessian" class="org.springframework.remoting.caucho.HessianServiceExporter">
    	<property name="service" ref="userService"/>
    	<property name="serviceInterface" value="com.kongur.monolith.weixin.client.user.RemoteUserService"/>
	</bean>
   
</beans>